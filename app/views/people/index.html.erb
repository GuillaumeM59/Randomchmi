<p id="notice"><%= notice %></p>

<h1>Listing People</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Group</th>
      <th>Sensei</th>

    </tr>
  </thead>

  <tbody>
    <% @people.each do |person| %>
      <tr>
        <td><%= person.name %></td>
        <td><%= person.group_id %></td>
        <td><%= person.sensei %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
